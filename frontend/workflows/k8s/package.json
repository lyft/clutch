{
  "name": "@clutch-sh/k8s",
  "version": "4.0.0-beta",
  "description": "Clutch K8s Workflows",
  "license": "Apache-2.0",
  "author": "clutch@lyft.com",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist && rm -f tsconfig.tsbuildinfo",
    "compile": "tsc -b",
    "compile:dev": "esbuild --target=es2019 --outdir=dist --sourcemap src/*.tsx",
    "compile:watch": "yarn compile:dev --watch=forever",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "publishBeta": "../../../tools/publish-frontend.sh k8s",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --collect-coverage",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@clutch-sh/api": "^4.0.0-beta",
    "@clutch-sh/core": "^4.0.0-beta",
    "@clutch-sh/data-layout": "^4.0.0-beta",
    "@clutch-sh/wizard": "^4.0.0-beta",
    "@emotion/styled": "^11.1.5",
    "@hookform/resolvers": "2.8.8",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.5",
    "lodash": "4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.3",
    "react-timeago": "^7.0.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@clutch-sh/tools": "^4.0.0-beta"
  },
  "engines": {
    "node": "<19"
  }
}
