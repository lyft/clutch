  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && rm -f tsconfig.tsbuildinfo",
    "compile": "yarn run -T tsc -b",
    "compile:dev": "yarn workspace @clutch-sh/tools buildDev $INIT_CWD",
    "compile:watch": "yarn compile:dev -w",
    "lint": "yarn run package:lint",
    "lint:fix": "yarn run lint --fix",
    "publishBeta": "../../../tools/publish-frontend.sh dynamodb",
    "test": "yarn run package:test",
    "test:coverage": "yarn run test --collect-coverage",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "@clutch-sh/api": "workspace:^",
    "@clutch-sh/core": "workspace:^",
    "@clutch-sh/data-layout": "workspace:^",
    "@clutch-sh/wizard": "workspace:^",
    "lodash": "^4.17.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@clutch-sh/tools": "workspace:^"
  },
  "peerDependencies": {
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=18 <19",
    "yarn": "^4.1.1"
  },
  "packageManager": "yarn@4.1.1",
  "stableVersion": "3.0.0-beta"
