{
  "name": "@clutch-sh/wizard",
  "version": "4.0.0-beta",
  "description": "Wizard Components to drive frontend workflows",
  "homepage": "https://clutch.sh/docs/development/frontend/overview#clutch-shwizard",
  "license": "Apache-2.0",
  "author": "clutch@lyft.com",
  "main": "dist/index.js",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist && rm -f tsconfig.tsbuildinfo",
    "compile": "tsc -b",
    "compile:dev": "yarn workspace @clutch-sh/tools buildDev $PWD",
    "compile:watch": "yarn compile:dev -w",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "prepublishOnly": "yarn run build",
    "publishBeta": "../../../tools/publish-frontend.sh wizard",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn run test --collect-coverage",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "@clutch-sh/core": "workspace:^",
    "@clutch-sh/data-layout": "workspace:^",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.5",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2"
  },
  "devDependencies": {
    "@clutch-sh/tools": "workspace:^"
  },
  "engines": {
    "node": "<19",
    "yarn": "^4.1.0"
  }
}
