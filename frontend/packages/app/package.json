{
  "name": "@clutch-sh/app",
  "version": "4.0.0-beta",
  "private": true,
  "description": "Clutch Application Components",
  "license": "Apache-2.0",
  "author": "clutch@lyft.com",
  "type": "module",
  "scripts": {
    "build": "yarn register-workflows && vite build",
    "clean": "rm -rf build",
    "lint": "yarn register-workflows && yarn run package:lint",
    "lint:fix": "yarn run lint --fix",
    "register-workflows": "yarn run workspace:registerWorkflows $INIT_CWD/src",
    "start": "yarn register-workflows && DANGEROUSLY_DISABLE_HOST_CHECK=true GENERATE_SOURCEMAP=false vite",
    "test": "yarn register-workflows && yarn run package:test",
    "test:coverage": "yarn run test --collect-coverage",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@clutch-sh/core": "workspace:^",
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@clutch-sh/tools": "workspace:^",
    "cypress": "9.7.0",
    "esbuild-loader": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=18 <19",
    "yarn": "^4.3.1"
  },
  "packageManager": "yarn@4.3.1",
  "proxy": "http://localhost:8080",
  "stableVersion": "3.0.0-beta"
}
