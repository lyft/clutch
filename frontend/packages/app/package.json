{
  "name": "@clutch-sh/app",
  "version": "4.0.0-beta",
  "private": true,
  "description": "Clutch Application Components",
  "license": "Apache-2.0",
  "author": "clutch@lyft.com",
  "scripts": {
    "build": "yarn register-workflows && react-app-rewired build",
    "clean": "rm -rf build",
    "eject": "react-scripts eject",
    "lint": "yarn register-workflows && eslint --ext .js,.jsx .",
    "lint:fix": "yarn run lint --fix",
    "register-workflows": "yarn workspace @clutch-sh/tools registerWorkflows $PWD/src",
    "start": "yarn register-workflows && GENERATE_SOURCEMAP=false react-app-rewired start",
    "test": "yarn register-workflows && jest --passWithNoTests",
    "test:coverage": "yarn run test --collect-coverage",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@clutch-sh/core": "workspace:^",
    "react-app-rewired": "^2.1.8"
  },
  "devDependencies": {
    "@clutch-sh/tools": "workspace:^",
    "@types/eslint": "^8",
    "esbuild-loader": "^3.0.0",
    "eslint": "^8.3.0",
    "jest": "^27.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": "<19",
    "yarn": "^4.1.1"
  },
  "packageManager": "yarn@4.1.1",
  "stableVersion": "3.0.0-beta",
  "proxy": "http://localhost:8080"
}
