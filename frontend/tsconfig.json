{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "@clutch-sh/*": ["api/*", "packages/*/src", "workflows/*/src"]
    }
  },
  "exclude": [
    "node_modules",
    "**/*.stories.ts*",
    "**/*.stories.js*",
    "**/*.test.js*",
