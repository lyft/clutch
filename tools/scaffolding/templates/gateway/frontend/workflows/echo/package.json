{
  "name": "@{{ .RepoName }}/echo",
  "version": "0.0.0",
  "private": true,
  "description": "Echo workflow",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "BABEL_ENV=build babel src --out-dir dist --source-maps --extensions .js,.jsx --no-comments",
    "compile:watch": "yarn run compile --watch",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "yarn run lint --fix",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn run test --collect-coverage",
    "test:watch": "yarn run test --watch",
    "upgrade": "yarn upgrade"
  },
  "dependencies": {
    "@clutch-sh/core": "^1.0.0-beta",
    "@clutch-sh/data-layout": "^1.0.0-beta",
    "@clutch-sh/wizard": "^1.0.0-beta",
    "@material-ui/core": "^4.11.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": "^17.0.0"
  },
  "devDependencies": {
    "@clutch-sh/tools": "^1.0.0-beta"
  }
}
